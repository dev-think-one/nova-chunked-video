{
    "name": "yaroslawww/nova-chunked-video",
    "description": "A highly targeted package for downloading video in chunks.",
    "keywords": [
        "laravel-nova",
        "nova",
        "video",
        "upload",
        "chunks"
    ],
    "homepage": "https://github.com/yaroslawww/nova-chunked-video",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "psalm/plugin-laravel": "^1.5",
        "vimeo/psalm": "^4.13"
    },
    "autoload": {
        "psr-4": {
            "Thinkone\\ChunkedVideo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thinkone\\ChunkedVideo\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
